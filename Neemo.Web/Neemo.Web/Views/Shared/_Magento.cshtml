    <script type="text/javascript">
        var neemo = neemo || {};
        neemo.endpoints = {
            cart: {
                addProduct: '@Url.Action("AddProduct", "Cart")',
                removeProduct: '@Url.Action("RemoveProduct", "Cart")',
                getItems: '@Url.Action("GetCurrentItems", "Cart")',
                updateQuantity: '@Url.Action("UpdateQuantity", "Cart")',
                calculateShippingEstimate: '@Url.Action("CalculateShippingEstimate", "Cart")',
                getShippingEstimate: '@Url.Action("GetShippingEstimate", "Products")'
            },
            img: {
                generic: '@Url.Image("placeholder")',
                get: function (fileName) {
                    if (fileName === undefined || fileName === null) {
                        return "";
                    }

                    var fileNameWithoutExtension = WithoutFileExtension(fileName);
                    var url = neemo.endpoints.img.generic.replace('placeholder', fileNameWithoutExtension);
                    return url;

                    function WithoutFileExtension(val) {
                        if (val.indexOf('.') === -1) {
                            return val;
                        }

                        return val.substring(0, val.lastIndexOf('.'));
                    }
                }
            },
            product: {
                generic: '@Url.Product(-1)',
                get: function (id) {
                    return neemo.endpoints.product.generic.replace(-1, id);
                },
                search: '@Url.Action("Find", "Products")'
            }
        };
    </script>

